string reverseWord(string str)
{
   int n = str.length(); 
  int temp[n];
    // Swap character starting from two 
    // corners 
    for(int i=0;i<n/2;i++)
    {   
        temp[i]=str[i];                              //swap the elements
        str[i]=str[n-i-1];
        str[n-i-1]=temp[i];
    }
    
    //or you can use swap fuction swap(str[i],str[n-i-1]) 
    
    for(int i=0;i<n;i++)
    {
        cout<<str[i];
    }
}
