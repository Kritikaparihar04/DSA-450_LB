 
brute approach
--------------------------------
 
 #include<iostream>
 using namespace std;
 int minJumps(int arr[], int n){
    // Your code here
    int c=0, i=0;
    while(i<n-1)
    {
        
        i=i+arr[i];
        c++;
    }
     return c;
    
}
 int main()
 {
     int arr[]={   1 ,3 ,5 ,8, 9 ,2 ,6 ,7 ,6 ,8 ,9  };
     int n=11;
     cout<<minJumps(arr,n);
 }
